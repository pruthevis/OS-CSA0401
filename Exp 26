#include <stdio.h>
#include <stdlib.h>
int main() {
    FILE *fp = fopen("data.txt", "w");
    if (!fp) {
        perror("Create");
        return 1;
    }
    fprintf(fp, "File Management Test\n");
    fclose(fp);
    fp = fopen("data.txt", "r");
    char buffer[100];
    fgets(buffer, sizeof(buffer), fp);
    printf("Read: %s", buffer);
    fclose(fp);
    if (remove("data.txt") == 0)
        printf("File deleted successfully.\n");
    else
        perror("Delete");

    return 0;
}
